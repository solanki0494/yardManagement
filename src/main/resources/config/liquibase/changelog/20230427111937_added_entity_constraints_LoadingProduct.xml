<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity LoadingProduct.
    -->
    <changeSet id="20230427111937-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="loading_product"
                                 constraintName="fk_loading_product__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="loading_id"
                                 baseTableName="loading_product"
                                 constraintName="fk_loading_product__loading_id"
                                 referencedColumnNames="id"
                                 referencedTableName="loading"/>
    </changeSet>
</databaseChangeLog>
